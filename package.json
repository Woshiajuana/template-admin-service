{
  "name": "wow-egg",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-wow-admin-framework": "0.0.2",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "ms": "^2.1.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-command start --daemon --title=template-admin-service --framework=egg-wow-admin-framework",
    "stop": "egg-command stop --title=template-admin-service",
    "build": "gulp && yarn run apidoc",
    "dev": "egg-bin dev --framework=../lib/egg-wow-admin-framework",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "apidoc": "apidoc -i app/controller -o app/public/apidoc/",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ajuan",
  "license": "MIT"
}
